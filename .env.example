# GitHub classic personal access token with repo scope (For cloning and creating PRs)
GITHUB_TOKEN=ghp_your_github_token_here

# Environments: development, test, production
NODE_ENV=development

# Gemini API Key (For generating specs)
GEMINI_API_KEY=your_gemini_api_key_here

# (Optional) SQLite DB path, defaults to database.sqlite in root
# DB_PATH=database.sqlite
